{
  "coverage-gutters.showLineCoverage": true,
  "coverage-gutters.noHighlightLight": "rgba(220, 50, 47, 0.1)",
  "coverage-gutters.highlightlight": "rgba(133, 153, 0, 0.1)",
  "coverage-gutters.showGutterCoverage": false,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.autoClosingBrackets": "never",
  "editor.autoClosingQuotes": "never",
  "editor.detectIndentation": false,
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.minimap.enabled": false,
  "editor.parameterHints.enabled": true,
  "editor.rulers": [80],
  "editor.tabSize": 4,
  "[fish]": {
    "editor.tabSize": 2
  },
  "[html]": {
    "editor.tabSize": 2
  },
  "[javascript]": {
    "editor.tabSize": 2
  },
  "[json]": {
    "editor.tabSize": 2
  },
  "[rust]": {
    "editor.rulers": [100]
  },
  "[shellscript]": {
    "editor.tabSize": 2
  },
  "[terraform]": {
    "editor.tabSize": 2
  },
  "[typescript]": {
    "editor.tabSize": 2
  },
  "[vue]": {
    "editor.tabSize": 2
  },
  "[yaml]": {
    "editor.tabSize": 2
  },
  "emmet.excludeLanguages": [],
  "emmet.includeLanguages": {
    "markdown": "html"
  },
  "emmet.triggerExpansionOnTab": true,
  "explorer.autoReveal": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 0,
  "extensions.ignoreRecommendations": true,
  "files.exclude": {},
  "files.insertFinalNewline": true,
  "files.watcherExclude": {
    "**/__pycache__/**": true,
    "**/_target/**": true,
    "**/.benchmarks/**": true,
    "**/.dvc/**": true,
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.hypothesis/**": true,
    "**/.mypy_cache/**": true,
    "**/.tox/**": true,
    "**/.venv/**": true,
    "**/node_modules/**": true,
    "**/target/**": true
  },
  "jupyter.alwaysTrustNotebooks": true,
  "jupyter.askForKernelRestart": false,
  "latex-workshop.bind.enter.key": false,
  "latex-workshop.latex.autoBuild.run": "onFileChange",
  "latex-workshop.view.pdf.viewer": "tab",
  "powershell.integratedConsole.showOnStartup": false,
  "python.formatting.provider": "black",
  "python.languageServer": "Pylance",
  "python.linting.flake8Args": ["--max-line-length=80"],
  "python.linting.flake8Enabled": true,
  "python.linting.mypyEnabled": true,
  "python.linting.pylintEnabled": false,
  "python.pythonPath": "${workspaceFolder}/.venv",
  "python.showStartPage": false,
  "python.testing.promptToConfigure": false,
  "python.testing.pytestEnabled": true,
  "python.testing.pytestPath": "tests",
  "rust-analyzer.inlayHints.enable": false,
  "rust-analyzer.updates.askBeforeDownload": false,
  "tabnine.experimentalAutoImports": true,
  "telemetry.enableTelemetry": false,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.fontFamily": "Fira Code",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.inheritEnv": true,
  "terminal.integrated.shell.linux": "/usr/bin/fish",
  "vsicons.dontShowNewVersionMessage": true,
  "workbench.colorTheme": "Solarized Light",
  "workbench.iconTheme": "vscode-icons",
  "workbench.startupEditor": "welcomePage",
  "workbench.statusBar.visible": true
}
