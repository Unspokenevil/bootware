{
  "docker.showStartPage": false,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.autoClosingBrackets": "never",
  "editor.autoClosingQuotes": "never",
  "editor.detectIndentation": false,
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.fontSize": {{ font_size }},
  "editor.minimap.enabled": false,
  "editor.parameterHints.enabled": true,
  "editor.rulers": [80],
  "editor.tabSize": 4,
  "[css]": {
    "editor.tabSize": 2
  },
  "[dockercompose]": {
    "editor.tabSize": 2
  },
  "[fish]": {
    "editor.tabSize": 2
  },
  "[html]": {
    "editor.tabSize": 2
  },
  "[javascript]": {
    "editor.tabSize": 2
  },
  "[json]": {
    "editor.tabSize": 2
  },
  "[jsonc]": {
    "editor.tabSize": 2
  },
  "[ruby]": {
    "editor.tabSize": 2
  },
  "[rust]": {
    "editor.rulers": [100]
  },
  "[shellscript]": {
    "editor.tabSize": 2
  },
  "[terraform]": {
    "editor.tabSize": 2
  },
  "[typescript]": {
    "editor.tabSize": 2
  },
  "[vue]": {
    "editor.tabSize": 2
  },
  "[xml]": {
    "editor.tabSize": 2
  },
  "[yaml]": {
    "editor.tabSize": 2
  },
  "emmet.excludeLanguages": [],
  "emmet.includeLanguages": {
    "markdown": "html"
  },
  "emmet.triggerExpansionOnTab": true,
  "explorer.autoReveal": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 0,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "*.bats": "shellscript",
    "rust-toolchain": "toml"
  },
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/*.olean": true,
    "**/CVS": true
  },
  "files.insertFinalNewline": true,
  "files.watcherExclude": {
    "**/__pycache__/**": true,
    "**/_target/**": true,
    "**/.benchmarks/**": true,
    "**/.dvc/**": true,
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.hypothesis/**": true,
    "**/.mypy_cache/**": true,
    "**/.tox/**": true,
    "**/.venv/**": true,
    "**/build/**": true,
    "**/node_modules/**": true,
    "**/target/**": true,
    "**/venv/**": true
  },
  "gitlens.showWelcomeOnInstall": false,
  "jupyter.alwaysTrustNotebooks": true,
  "jupyter.askForKernelRestart": false,
  "latex-workshop.bind.enter.key": false,
  "latex-workshop.latex.autoBuild.run": "onFileChange",
  "latex-workshop.view.pdf.viewer": "tab",
  "liveServer.settings.donotShowInfoMsg": true,
  "powershell.integratedConsole.showOnStartup": false,
  "python.formatting.provider": "black",
  "python.languageServer": "Pylance",
  "python.linting.flake8Args": ["--max-line-length=80"],
  "python.linting.flake8Enabled": true,
  "python.linting.mypyEnabled": true,
  "python.linting.pylintEnabled": false,
  "python.pythonPath": "${workspaceFolder}/.venv",
  "python.showStartPage": false,
  "python.testing.promptToConfigure": false,
  "python.testing.pytestEnabled": true,
  "python.testing.pytestPath": "tests",
  "redhat.telemetry.enabled": false,
{% if vscode_extensions | length > 0 %}
  "remote.SSH.defaultExtensions": [
{% for extension in vscode_extensions[:-1] %}
    "{{ extension }}",
{% endfor %}
    "{{ vscode_extensions[-1] }}"
  ],
{% endif %}
  "remote.SSH.useLocalServer": false,
  "rust-analyzer.inlayHints.enable": false,
  "rust-analyzer.notifications.cargoTomlNotFound": false,
  "rust-analyzer.updates.askBeforeDownload": false,
  "security.workspace.trust.enabled": false,
  "tabnine.experimentalAutoImports": true,
  "telemetry.enableTelemetry": false,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.fontFamily": "Fira Code",
  "terminal.integrated.fontSize": {{ font_size }},
  "terminal.integrated.inheritEnv": true,
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "source": "PowerShell",
      "args": ["-NoLogo"]
    }
  },
  "update.mode": "none",
  "vsicons.dontShowNewVersionMessage": true,
  "workbench.colorTheme": "Solarized Light",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "workbench.iconTheme": "vscode-icons",
  "workbench.startupEditor": "welcomePage",
  "workbench.statusBar.visible": true
}
